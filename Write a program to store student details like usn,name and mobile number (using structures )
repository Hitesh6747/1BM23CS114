#include<stdio.h>
struct student
    {
        char name[100];
        int phone;
        char usn[20];
    };
    int main (){
        struct student s;

        printf("enter your name ");
        scanf("%s",s.name);
        printf("enter your phone number");
        scanf("%d",&s.phone);
        printf("enter your usn ");
        scanf("%s",s.usn);

        printf("-----------student details------------\n");
        printf("Name:%s\n",s.name);
        printf("Phone No.:%d\n",s.phone);
        printf("Usn:%s\n",s.usn);

    }
